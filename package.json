{
	"name": "vscode-resx-editor",
	"displayName": "ResX Editor",
	"description": "ResX Editor",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.49.0"
	},
	"publisher": "DominicVonk",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:resx.editor"
	],
	"icon": "resx.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/DominicVonk/vscode-resx.git"
	},
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "resx.editor",
				"displayName": "ResX",
				"selector": [
					{
						"filenamePattern": "*.resx"
					}
				],
				"priority": "default"
			}
		],
		"languages": [
			{
				"id": "xml",
				"extensions": [
					".resx"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"resx": "^2.0.3"
	}
}